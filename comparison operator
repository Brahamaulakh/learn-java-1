public class Main {
    public static void main(String[] args){
       int x = 1;
       int y = 2;
        System.out.println(x==y);//false as they are not equal
        System.out.println(x!=y);//true cuz both are not equal
        System.out.println(x<=y);//true 
        System.out.println(x>=y);//false
    }

    }
